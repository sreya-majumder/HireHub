{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/me/page.jsx"
, "src/app/recruiter/profile/[id]/page.jsx", "src/app/(home-page)/recruiter-home/page.jsx", "src/app/(home-page)/applicant-home/page.jsx", "src/app/profile/[id]/page.jsx", "src/app/applicants/recruiter/page.jsx"  ],
  "exclude": ["node_modules"]
}
